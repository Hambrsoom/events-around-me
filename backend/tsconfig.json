{
    "compilerOptions": {
      "lib": [
        "es5",
        "es6"
     ],
     "typeRoots" : ["./node_modules/@types", "./typings"],
      "module": "commonjs",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
